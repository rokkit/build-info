<div id="login-block" class="block login">
	<% if user_signed_in? %>
		<h2 class="block-title">Меню пользователя</h2>
		<div class="content">
			<%= render_navigation context: :user %>
		</div>
	<% else %>
		<%= render '/devise/sessions/new' %>
	<% end %>
</div>
<div class="banner">
	<object type="application/x-shockwave-flash" data="<%= asset_path '200h300_legenda_root_30_bn_02.swf'%>" 
	    width="400" height="550" align="top"> 
	    <param name="movie" value="<%= asset_path '200h300_legenda_root_30_bn_02.swf'%>" />
	    <param name="quality" value="high" />
	    <param name="align" value="t" />
	</object>
</div>