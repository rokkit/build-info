<% content_for :page_title do %>Агенства недвижимости<% end %>
<%= link_to 'Создать агенство', new_agency_path if can? :create, Agency %>

<table>
  <tr>
    <th>Агенство</th>
    <th>Описание</th>
  </tr>

<% @agencies.each do |agency| %>
  <tr>
    <td><%= link_to agency, agency %></td>
    <td><%= agency.description %></td>
  </tr>
<% end %>
</table>

<br />

