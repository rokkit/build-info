<%= simple_form_for(@reester_object) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :reester_id %>
    <%= f.input :cadaster_number %>
    <%= f.input :address %>
    <%= f.input :apartement_number %>
    <%= f.input :area %>
    <%= f.input :cost %>
    <%= f.input :cost_one_meter %>
    <%= f.association :distinct %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
